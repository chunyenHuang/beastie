.keypad {
    margin: 10px auto;
    padding: 0;
    width: 300px; /* not actually a static size, % in real use */
    height: 400px;
}

.keypad-display
    padding: 5px
    background-color: #f5f5f5
    height: 50px

table.keypad tr td
    text-align: center
    padding: 10px

table.keypad .md-button
    font-size: 20px
    width: 80px
    height: 80px
