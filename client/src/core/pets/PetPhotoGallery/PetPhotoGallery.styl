#pet-photo-gallery
    md-grid-tile .gallery-img
        background-repeat no-repeat
        background-position: center center;
        -webkit-background-size: contain;
        -moz-background-size: contain;
        -o-background-size: contain;
        background-size contain
    .tile-gallery-img
        img
            max-width: 100%
            height: auto
#preview-large
    width 100%
    height 100%

    background-color rgba(0, 0, 0, 0.75)
    background-repeat no-repeat
    background-position: center center;
    -webkit-background-size: contain;
    -moz-background-size: contain;
    -o-background-size: contain;
    background-size contain
    img
        height: 100%
        width: auto
        margin: auto;

#preview-large::after
    width 100%
    height 100%
    background red
