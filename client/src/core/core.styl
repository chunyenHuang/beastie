@import "~variable"


#core
    md-dialog
        md-subheader
            margin-bottom: 15px
        max-height: 600px
        md-input-container
            margin 0
            margin-left: 20px
            padding-left: 20px
    .sidenav
        // background-color: $bgColor
        margin-top: -30px
        padding: 30px
        md-icon
            display: block
            margin: 50px 0px 0px 0px
            transform: scale(2, 2);
            color: #fff2d6
        md-icon:hover
            color: #ffffff
        md-icon:focus
            outline: none
            border: 0

.padding-6-0
    padding 6px 0px

.md-button
    font-weight 600
    font-size 16px
    border none
.border-solid
    border solid 2px