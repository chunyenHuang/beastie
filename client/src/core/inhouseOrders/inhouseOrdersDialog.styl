@import "~variable"
#inhouse-orders-dialog
    md-dialog
        min-height: 650px
        min-width: 1000px
#inhouse-orders
    md-toolbar
        min-height: 30px
    .header
        width: 100%
        height: 30px
        h5
            margin: 0px
            padding: 0px
        md-icon
            margin-top: 0px
    .footer
        width: 700px
        padding: 20px
        border-top: 1px solid #d2d2d2
    button
        border: 1px solid #6c6c6c
    .padding-box
        padding-bottom: 10px
        h3
            margin-bottom: 0px

#inhouse-orders-canvas
    width: 500px
    // height: 500px
    border 1px solid #1b1b1b
